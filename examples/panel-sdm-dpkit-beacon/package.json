    "start:tuya": "ray start -t tuya --verbose",
    "build:native": "ray build -t native --verbose",
    "readResource": "node scripts/read.js",
    "build:tuya": "ray build -t tuya --verbose"
  },
  "dependencies": {
    "@ray-js/battery": "^0.0.5",
    "@ray-js/components-ty-perf-text": "^0.0.6",
    "@ray-js/components-ty-slider": "^0.2.47",
    "@ray-js/lamp-circle-picker": "^1.0.4",
    "@ray-js/panel-sdk": "^1.12.0",
    "@ray-js/ray": "^1.5.22",
    "@ray-js/ray-error-catch": "^0.0.19",
    "@ray-js/svg": "^0.1.8",
    "base64-browser": "^1.0.1",
    "clsx": "^1.2.1",
    "color": "^4.2.3",
    "core-js": "^3.23.5",
    "lodash-es": "^4.17.21",
    "react-redux": "^7.0.0",
    "redux": "^4.2.0",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@ray-js/cli": "^1.4.0",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "eslint-config-tuya-panel": "^0.4.1",
    "eslint-plugin-literal-check": "^0.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.2.0",
    "lint-staged": "^10.2.11",
    "prettier": "^1.16.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.4.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --config commitlint.config.js",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "@types/react": "^17.0.24",
    "follow-redirects": "1.15.6"
  }
}
